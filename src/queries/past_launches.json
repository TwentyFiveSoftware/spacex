{
    "query":{
        "upcoming": false
    },
    "options": {
        "sort": {
            "flight_number": "desc"
        },
        "select": {
            "_id": 1,
            "name": 1,
            "flight_number": 1,
            "details": 1,
            "date_unix": 1,
            "date_precision": 1,
            "tbd": 1,
            "net": 1,
            "rocket": 1,
            "payloads": 1,
            "launchpad": 1,
            "cores.flight": 1,
            "cores.landing_attempt": 1,
            "cores.landing_type": 1,
            "cores.landpad": 1
