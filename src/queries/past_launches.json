        "populate": [
            {
                "path": "rocket",
                "select": {
                    "_id": 0,
                    "name": 1,
                    "type": 1,
                    "height.meters": 1,
                    "cost_per_launch": 1,
                    "description": 1
                }
            },
            {
                "path": "payloads",
                "select": {
                    "_id": 0,
                    "name": 1,
                    "type": 1,
                    "customers": 1,
                    "nationalities": 1,
                    "manufacturers": 1,
