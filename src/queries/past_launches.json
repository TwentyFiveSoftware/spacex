            {
                "path": "launchpad",
                "select": {
                    "_id": 0,
                    "name": 1,
                    "full_name": 1,
                    "locality": 1,
                    "region": 1,
                    "details": 1
                }
            },
            {
                "path": "cores.landpad",
                "select": {
                    "_id": 0,
                    "name": 1,
                    "full_name": 1,
                    "locality": 1,
                    "region": 1,
